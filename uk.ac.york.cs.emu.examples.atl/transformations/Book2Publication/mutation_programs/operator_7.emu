//7) OutPatternElement Class Change: OCL.VariableDeclaration_type_replacement
@action replace
@feature type
@type SimpleOutPatternElement
pattern mutation_operator_7
instance:SimpleOutPatternElement,
possible_type:OclModelElement from: OclModelElement.all.select(e| e.name <> instance.type.name )
{
	do {
		var new_type = OclModelElement.createInstance();
		new_type.`model` = possible_type.`model`;
