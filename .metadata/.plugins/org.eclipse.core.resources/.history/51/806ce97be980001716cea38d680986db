-- This mutation just reference a library file that is not even exist or included
-- in the launch
@action add
@feature libraries
pattern Unit_LibraryRef_libraries_add
instance:Unit
guard: instance.isTypeOf(Module) and instance.name = "Book2Publication"
{
	do {
		var lib = LibraryRef.createInstance();
		lib.name = "Ref_Library";
		instance.libraries.add(lib);
	}
}