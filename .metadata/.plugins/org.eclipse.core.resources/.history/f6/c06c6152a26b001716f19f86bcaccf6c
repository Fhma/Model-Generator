var selection:Sequence;
var allOperators = MutationOperator.all.select(e|e.isTypeOf(AddMutationOperator) and );

("All addition mutation operators: "+allOperators.size()).println();
while(selection.size()<=10)
{
	var pick = allOperators.random();
	if(not selection.includes(pick))
	{
		selection.add(pick.signature);
	}	
}

("Selected operators: "+selection).println();