var selection:Sequence;
// retrive all operators
var allOperators = MutationOperator.all.select(e|e.isTypeOf(AddMutationOperator) and e.op_set.containingClass.startsWith("ATL"));

// read old selected operators
var fr = new Native("java.io.FileReader")("selected_operators.txt");

var br = new Native("java.io.BufferedReader")(fr);

var sign:String;

sign = br.readLine();

while(sign.isDefined() and sign.length>0) { 
	selection.add(sign);
	"".println(sign);
	sign = br.readLine();
}
fr.close;

selection.size.println();

var fw = new Native("java.io.FileWriter")("selected_operators.txt");

var loop:Integer = 1;

while(loop<=10)
{
	var pick = allOperators.random();
	if(not selection.includes(pick))
	{
		selection.add(pick.signature);
		loop = loop + 1;
	}
}

fw.close;
