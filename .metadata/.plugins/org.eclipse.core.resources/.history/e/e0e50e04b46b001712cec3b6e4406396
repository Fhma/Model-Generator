var selection:Sequence;
var allOperators = MutationOperator.all.select(e|e.isTypeOf(AddMutationOperator) and e.op_set.containingClass.startsWith("ATL"));

("All addition mutation operators: "+allOperators.size()).println();

var sign:String;

while(selection.size()<=10)
{
	var pick = allOperators.random();
	if(not selection.includes(pick))
	{
		sign = pick.op_set.containingClass + ":" + pick.signature;
		selection.add(sign);
	}	
}

var file = new Native("java.io.File")("select_operators.txt");